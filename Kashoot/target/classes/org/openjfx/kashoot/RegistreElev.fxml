            </ImageView>
         </children>
      </AnchorPane>
      <TextField fx:id="txtUsername1" layoutX="505.0" layoutY="254.0" promptText="Username" style="-fx-background-color: transparent; -fx-border-color: black; -fx-border-width: 0px 0px 1px 0px;" />
